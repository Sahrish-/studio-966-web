%html
  %head
    %meta{:charset => "utf-8"}/
    %meta{:content => "IE=edge;chrome=1", "http-equiv" => "X-UA-Compatible"}/

    %title= current_page.data.title || "966.io Studios | We Are The Bes!"

    -# Set the viewport width to device width for mobile
    %meta{ name: "viewport", content: "width=device-width, initial-scale=1.0" }

    %link{ rel: "stylesheet", href: "http://fonts.googleapis.com/css?family=Source+Sans+Pro: 400, 600. 400italic", type:"text/css" }

    %link{rel: "stylesheet", href: "http://fonts.googleapis.com/css?family=Source+Sans+Pro|Arvo|PT+Sans|Exo:300|Vollkorn'", type: "ext/css" }

    =stylesheet_link_tag "application"

  %body{ class: page_classes }
    = partial "layouts/unsupported_browsers"
    = partial "layouts/header"
    = yield
    = partial "layouts/footer"

    = javascript_include_tag_ "all"
